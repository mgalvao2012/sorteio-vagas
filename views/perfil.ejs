        <div class="container" style="padding-top: 30px; padding-bottom: 30px;">
            <div class="sign-up-agileinfo" >
                <h3 class="titulo">Atualização de Perfil</h3>
                <p><%= mensagem %></p>
                <form action="/perfil" method="POST">
                    <p class="rotulo_campos">Nome</p>
                    <input type="text" name="name" value="<%= name %>" required="">
                    <p class="rotulo_campos">Email</p>
                    <input type="text" name="email" value="<%= email %>" required="">
                    <p class="rotulo_campos">Data de nascimento</p>
                    <input type="date" name="data_nascimento" value="">
                    <p class="rotulo_campos">Unidade (formato: T9A999)</p>
                    <input type="text" name="unidade" value="<%= unidade %>" required="">
					<p class="rotulo_campos">Pessoa com necessidade especial 
						<% if (pne === 'on') { %>
							<input type="checkbox" name="pne" checked="checked" style="height: 25px; width: 25px;">
						<% } else { %>
							<input type="checkbox" name="pne" style="height: 25px; width: 25px;">
						<% } %>
					</p>
<!--					
                    <p class="rotulo_campos">Senha</p>
                    <input type="password" class="password" name="password" >
                    <p class="rotulo_campos">Confirmação da senha</p>
                    <input type="password" class="password" name="confirm password" >
-->					
                    <input type="submit" value="Atualizar">
                </form>
            </div>            
